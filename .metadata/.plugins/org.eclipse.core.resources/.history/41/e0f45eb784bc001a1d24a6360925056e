	BGIWAN3(3, "Restoran Bang Iwan" , "Nasi Goreng Ayam" , 17000), BGIWAN4(4, "Restoran Bang Iwan" , "Indomei Rebus Special" , 39000),
	OTOY1(1, "Restoran Otoy" , "Paket Ayam Komplit", 47000), OTOY2(2, "Restoran Otoy" , "Paket Empal Komplit", 49000),
	OTOY3(3, "Restoran Otoy" , "Semur Tempe dan Tahu", 7000), OTOY4(4, "Restoran Otoy" , "Ikan Mas Bakar", 22000),
	IbuAzka1(1, "Restoran Ibu Azka" , "Paket Ayam Paha Goreng", 23000), IbuAzka2(2, "Restoran Ibu Azka" , "Paket Ayam Dada Goreng", 24000),
	IbuAzka3(3, "Restoran Ibu Azka" , "Gurame Goreng", 27000), IbuAzka4(4, "Restoran Ibu Azka" , "Kerang Ijo Saos Padang", 25000),
